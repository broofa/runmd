{
  "name": "runmd",
  "author": {
    "name": "Robert Kieffer",
    "email": "robert@broofa.com"
  },
  "version": "1.3.6",
  "bin": {
    "runmd": "./cli.js"
  },
  "main": "./index.js",
  "description": "Runnable README files",
  "keywords": [
    "markdown",
    "readme"
  ],
  "license": "MIT",
  "homepage": "https://github.com/broofa/runmd",
  "ava": {
    "files": [
      "test/*.spec.js"
    ]
  },
  "lint-staged": {
    "**/*.js": "semistandard"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/broofa/runmd.git"
  },
  "dependencies": {
    "minimist": "1.2.6",
    "require-like": "0.1.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.0",
    "ava": "4.2.0",
    "commitlint": "17.0.0",
    "eslint": "8.15.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "npm-run-all": "4.1.5",
    "semistandard": "16.0.1"
  },
  "scripts": {
    "prepare": "husky install",
    "docs": "./cli.js --output README.md src/README_js.md",
    "test:lint": "semistandard",
    "test:unit": "ava",
    "test": "run-p docs test:lint test:unit && git diff --quiet"
  }
}
